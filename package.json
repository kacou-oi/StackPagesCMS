{
  "name": "stackpages-cms",
  "version": "1.0.0",
  "description": "A starter kit for a personal website with a dynamic blog, powered by Cloudflare Pages and a Substack RSS feed.",
  "main": "_worker.js",
  "scripts": {
    "start": "wrangler pages dev ./frontend --compatibility-date=2024-05-20 --experimental-local-workers",
    "deploy": "wrangler pages deploy ./frontend --project-name=stackpages-cms"
  },
  "devDependencies": {
    "wrangler": "^3.57.2"
  },
  "private": true
}
